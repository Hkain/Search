@model List<BusnesLogic.User>

@foreach (var user in Model)
{
    <li class="myLi" name="@user.Id" style="position:relative; border:2px;" onclick="editUser(@user.Id)">
        <div id="user-image" style="width:50px;height:50px;">
            @Html.Raw(user.UserImage != null && user.UserImage.Count() != 0 ? "<img style='width:50px; height:50px;' src=\"data:image/jpeg;base64," + Convert.ToBase64String(user.UserImage) + "\" />" :
                                                      "<img style='width:50px; height:50px;' src='/Content/Image/EmpAvatar.png'  />")
            <p style="left:70px; top:-30px ;position:relative">@user.Name</p>
        </div>
    </li>
}
